<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <chase-distance-m type="double" archive="y"> -30 </chase-distance-m>

  <view n="100">
    <name>Co Pilote</name>
    <type>lookfrom</type>
    <internal archive="y">true</internal>
    <config>
      <from-model type="bool">true</from-model>
      <from-model-idx type="int">0</from-model-idx>
      <ground-level-nearplane-m type="double">0.01f</ground-level-nearplane-m>
      <pitch-offset-deg>-5</pitch-offset-deg>
      <x-offset-m archive="y">  0.294 </x-offset-m>	<!-- Right -->
      <y-offset-m archive="y">  1.212 </y-offset-m>	<!-- Up    -->
      <z-offset-m archive="y"> -9.538 </z-offset-m>	<!-- Back  -->
      <pitch-offset-deg> -10 </pitch-offset-deg>
      <default-field-of-view-deg> 65 </default-field-of-view-deg>
    </config>
  </view>

  <view n="101">
    <name>Tail Camera View</name>
    <type>lookfrom</type>
    <internal archive="y">true</internal>
    <config>
      <from-model type="bool">true</from-model>
      <from-model-idx type="int">0</from-model-idx>
      <ground-level-nearplane-m type="double">0.01f</ground-level-nearplane-m>
      <default-field-of-view-deg type="double">30</default-field-of-view-deg>
      <pitch-offset-deg>-5</pitch-offset-deg>
      <x-offset-m archive="y"> 0.000 </x-offset-m> <!--Right-->
      <y-offset-m archive="y"> 2.760 </y-offset-m> <!--Up-->
      <z-offset-m archive="y"> 8.281 </z-offset-m> <!--Back-->
    </config>
  </view>

  <view n="102">
    <name>Roof Gunner</name>
    <type>lookfrom</type>
    <internal archive="y">true</internal>
    <config>
      <from-model type="bool">true</from-model>
      <from-model-idx type="int">0</from-model-idx>
      <ground-level-nearplane-m type="double">0.01f</ground-level-nearplane-m>
      <pitch-offset-deg>-5</pitch-offset-deg>
      <x-offset-m archive="y">  0.000 </x-offset-m>	<!-- Right -->
      <y-offset-m archive="y">  1.6 </y-offset-m>	<!-- Up    -->
      <z-offset-m archive="y">  1.2 </z-offset-m>	<!-- Back  -->
      <pitch-offset-deg> 0 </pitch-offset-deg>
      <heading-offset-deg>180</heading-offset-deg>
      <default-field-of-view-deg>55</default-field-of-view-deg>
    </config>
  </view>
  
  <view n="103">
    <name>Tail Gunner</name>
    <type>lookfrom</type>
    <internal archive="y">true</internal>
    <config>
      <from-model type="bool">true</from-model>
      <from-model-idx type="int">0</from-model-idx>
      <ground-level-nearplane-m type="double">0.01f</ground-level-nearplane-m>
      <pitch-offset-deg>-5</pitch-offset-deg>
      <x-offset-m archive="y">  0.000 </x-offset-m>	<!-- Right -->
      <y-offset-m archive="y">  0.7 </y-offset-m>	<!-- Up    -->
      <z-offset-m archive="y"> 12.8 </z-offset-m>	<!-- Back  -->
      <pitch-offset-deg> 0 </pitch-offset-deg>
      <heading-offset-deg>180</heading-offset-deg>
      <default-field-of-view-deg>55</default-field-of-view-deg>
    </config>
  </view>
  
  <view n="104">
    <name>Nose gunner</name>
    <type>lookfrom</type>
    <internal archive="y">true</internal>
    <config>
      <from-model type="bool">true</from-model>
      <from-model-idx type="int">0</from-model-idx>
      <ground-level-nearplane-m type="double">0.01f</ground-level-nearplane-m>
      <pitch-offset-deg>-5</pitch-offset-deg>
      <x-offset-m archive="y">  0 </x-offset-m>	<!-- Right -->
      <y-offset-m archive="y">  0 </y-offset-m>	<!-- Up    -->
      <z-offset-m archive="y"> -13 </z-offset-m>	<!-- Back  -->
      <pitch-offset-deg> 0 </pitch-offset-deg>
      <heading-offset-deg> 0 </heading-offset-deg>
      <default-field-of-view-deg>75</default-field-of-view-deg>
    </config>
  </view>
  
  <view n="105">
    <name>Bomb aimer</name>
    <type>lookfrom</type>
    <internal archive="y">true</internal>
    <config>
      <from-model type="bool">true</from-model>
      <from-model-idx type="int">0</from-model-idx>
      <ground-level-nearplane-m type="double">0.01f</ground-level-nearplane-m>
      <pitch-offset-deg>-5</pitch-offset-deg>
      <x-offset-m archive="y">  0 </x-offset-m>	<!-- Right -->
      <y-offset-m archive="y">  -1.180 </y-offset-m>	<!-- Up    -->
      <z-offset-m archive="y"> -12 </z-offset-m>	<!-- Back  -->
      <pitch-offset-deg> -15 </pitch-offset-deg>
      <heading-offset-deg> 0 </heading-offset-deg>
      <default-field-of-view-deg>75</default-field-of-view-deg>
    </config>
  </view>
  
  <view n="0">
    <internal archive="y">true</internal>
    <config>
      <x-offset-m archive="y"> -0.294 </x-offset-m>	<!-- Right -->
      <y-offset-m archive="y">  1.212 </y-offset-m>	<!-- Up    -->
      <z-offset-m archive="y"> -9.538 </z-offset-m>	<!-- Back  -->
      <pitch-offset-deg> -10 </pitch-offset-deg>
      <default-field-of-view-deg> 70 </default-field-of-view-deg>
    </config>
  </view>

  <view n="1">
    <config>
      <target-z-offset-m archive="y" type="double"> 0 </target-z-offset-m>
    </config>
  </view>

  <view n="2">
    <config>
      <target-z-offset-m archive="y" type="double"> 0 </target-z-offset-m>
    </config>
  </view>

  <view n="3">
    <config>
      <target-z-offset-m archive="y" type="double"> 0 </target-z-offset-m>
    </config>
  </view>

  <view n="4">
    <config>
      <target-z-offset-m archive="y" type="double"> 0 </target-z-offset-m>
    </config>
  </view>

  <view n="5">
    <config>
      <target-z-offset-m archive="y" type="double"> 0 </target-z-offset-m>
    </config>
  </view>

  <view n="6">
    <config>
      <target-z-offset-m archive="y" type="double"> 0 </target-z-offset-m>
    </config>
  </view>

</PropertyList>
